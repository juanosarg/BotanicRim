<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  
  <!-- ============= Growing ============= -->

  <WorkGiverDef>
    <defName>BR_HarvestBotany</defName>
    <label>harvest crops</label>
    <giverClass>BotanicRim.WorkGiver_GrowerHarvestBotany</giverClass>
    <workType>BR_Botany</workType>
    <priorityInType>100</priorityInType>
    <verb>harvest</verb>
    <gerund>harvesting</gerund>
    <scanThings>false</scanThings>
    <scanCells>true</scanCells>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>

  <WorkGiverDef>
    <defName>BR_SowBotany</defName>
    <label>sow crops</label>
    <giverClass>BotanicRim.WorkGiver_GrowerSowBotany</giverClass>
    <workType>BR_Botany</workType>
    <priorityInType>50</priorityInType>
    <verb>sow</verb>
    <gerund>sowing</gerund>
    <scanThings>false</scanThings>
    <scanCells>true</scanCells>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
  </WorkGiverDef>
  
  <!-- Workbenches -->
  
  	<WorkGiverDef>
		<defName>BR_DoBillsBotanyWB</defName>
		<label>works at botany workbench</label>
		<giverClass>WorkGiver_DoBill</giverClass>
		<workType>Crafting</workType>
		<priorityInType>100</priorityInType>
		<fixedBillGiverDefs>
			<li>BR_BotanyWorkbench</li>
		</fixedBillGiverDefs>
		<verb>work at botany workbench</verb>
		<gerund>working at botany workbench</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<prioritizeSustains>true</prioritizeSustains>
	</WorkGiverDef>
  
 

</Defs>